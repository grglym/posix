set fileencoding=utf-8
set fileencodings=utf-8,gb2312,gb18030,latin1
set termencoding=utf-8
set encoding=utf-8

syntax on

color evening

filetype on

filetype plugin on


set number

set cc=64

set cursorline

set tabstop=4

set softtabstop=4

set shiftwidth=4 

set autoindent

set smartindent

set cindent

set expandtab

inoremap jj <Esc>

imap { {<CR>}<Esc>kA<CR>



